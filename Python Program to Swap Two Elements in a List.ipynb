      "['1', '4', '3', '2']\n"
     ]
    }
   ],
   "source": [
    "def swap(l,pos1,pos2):\n",
    "    temp=l[pos1]\n",
    "    l[pos1]=l[pos2]\n",
    "    l[pos2]=temp\n",
    "    return l\n",
    "    \n",
    "l=list(input())\n",
    "pos1=int(input())\n",
    "pos2=int(input())\n",
    "print(swap(l,pos1,pos2))"
   ]
